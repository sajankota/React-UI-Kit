    "@babel/core": "^7.22.11",
    "@babel/plugin-syntax-flow": "^7.22.5",
    "@babel/plugin-transform-react-jsx": "^7.22.5",
    "cross-env": "^7.0.3",
    "eslint": "^8.48.0",
    "prettier": "^2.7.1",
    "rimraf": "^3.0.2",
    "typescript": "^2.7",
    "yarn-upgrade-all": "^0.7.1"
  }
}
